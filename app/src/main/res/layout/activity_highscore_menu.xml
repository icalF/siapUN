<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".HighscoreMenuActivity">


  <Space
    android:layout_width="280px"
    android:layout_height="200px"
    android:background="@drawable/round_rect_shape"
    android:layout_alignParentTop="true"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:id="@+id/indBox"/>

  <Space
    android:layout_width="280px"
    android:layout_height="200px"
    android:background="@drawable/round_rect_shape"
    android:layout_alignParentTop="true"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true"
    android:id="@+id/engBox"/>

  <Space
    android:layout_width="280px"
    android:layout_height="200px"
    android:background="@drawable/round_rect_shape"
    android:layout_below="@+id/indBox"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginTop="49dp"
    android:id="@+id/matBox"/>

  <Space
    android:layout_width="280px"
    android:layout_height="200px"
    android:background="@drawable/round_rect_shape"
    android:layout_alignTop="@+id/matBox"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true"
    android:id="@+id/fisBox"/>

  <Space
    android:layout_width="280px"
    android:layout_height="200px"
    android:background="@drawable/round_rect_shape"
    android:layout_marginTop="49dp"
    android:id="@+id/kimBox"
    android:layout_below="@+id/matBox"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"/>

  <Space
    android:layout_width="280px"
    android:layout_height="200px"
    android:background="@drawable/round_rect_shape"
    android:layout_alignTop="@+id/kimBox"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true"
    android:id="@+id/bioBox"/>
</RelativeLayout>
