<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:paddingBottom="@dimen/activity_vertical_margin"
  android:paddingLeft="@dimen/activity_horizontal_margin"
  android:paddingRight="@dimen/activity_horizontal_margin"
  android:paddingTop="@dimen/activity_vertical_margin"
  tools:context=".HighscoreMenuActivity"
  android:gravity="center">


  <Button
    android:layout_width="480px"
    android:layout_height="100px"
    android:background="@drawable/round_rect_shape"
    android:id="@+id/paket1h"
    android:text="Paket 1"/>

  <Button
    android:layout_width="480px"
    android:layout_height="100px"
    android:background="@drawable/round_rect_shape"
    android:id="@+id/paket2h"
    android:layout_below="@+id/paket1h"
    android:layout_alignLeft="@+id/paket1h"
    android:layout_alignStart="@+id/paket1h"
    android:layout_marginTop="20dp"
    android:text="Paket 2"/>

  <Button
    android:layout_width="480px"
    android:layout_height="100px"
    android:background="@drawable/round_rect_shape"
    android:id="@+id/paket3h"
    android:layout_below="@+id/paket2h"
    android:layout_alignLeft="@+id/paket2h"
    android:layout_alignStart="@+id/paket2h"
    android:layout_marginTop="20dp"
    android:text="Paket 3"/>

  <Button
    android:layout_width="480px"
    android:layout_height="100px"
    android:background="@drawable/round_rect_shape"
    android:id="@+id/paket4h"
    android:layout_below="@+id/paket3h"
    android:layout_alignLeft="@+id/paket3h"
    android:layout_alignStart="@+id/paket3h"
    android:layout_marginTop="20dp"
    android:text="Paket 4"/>

</RelativeLayout>
